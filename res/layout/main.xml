<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<Button android:layout_height="wrap_content"
		android:layout_gravity="center_horizontal" android:layout_width="wrap_content"
		android:id="@+id/buttonReg" android:text="@string/buttonText"
		android:layout_marginTop="10px"></Button>
	<Button android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:id="@+id/buttonUnReg" android:text="@string/buttonUnregText"></Button>
	<TextView android:layout_height="wrap_content" android:text="TextView"
		android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_width="wrap_content" android:paddingTop="10px"
		android:paddingLeft="20px" android:id="@+id/textStatus"></TextView>
</LinearLayout>
